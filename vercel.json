{
  "version": 2,
  "name": "tunehub-backend",
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/usersongs",
      "dest": "/route/usersongs.js"
    },
    {
      "src": "/api/userinfo",
      "dest": "/route/userinfo.js"
    },
    {
      "src": "/api/username",
      "dest": "/route/username.js"
    },
    {
      "src": "/api/admin",
      "dest": "/route/admin.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
